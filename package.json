        "uuid": "^11.0.3",
        "web-vitals": "^2.1.4"
    },
    "repository": "https://github.com/Abhi-Radadiya/peptide_oligo_synthesis",
    "publish": {
        "provider": "github",
        "releaseType": "release"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "el": "electron .",
        "dist": "electron-builder",
        "electron": "node starter.js"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "electron": "^31.1.0",
        "electron-builder": "^24.13.3",
        "electron-is-dev": "^3.0.1",
        "tailwindcss": "^3.4.4"
    },
    "build": {
        "appId": "com.peptide.synthesis",
        "productName": "Peptide_Oligo_Synthesis",
        "files": [
            "build/**/*",
            "main.js",
            "preload.js",
            "package.json"
        ],
        "directories": {
            "buildResources": "public"
        },
        "win": {
            "target": "nsis",
            "icon": "assets/icon.ico"
        },
        "extraResources": [
            {
                "from": "node_modules/serialport/",
                "to": "node_modules/serialport/"
            }
        ],
        "asar": true
    }
}
