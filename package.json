{
    "name": "Peptide & Oligo Synthesis",
    "version": "0.1.0",
    "private": true,
    "description": "Peptide & Oligo Synthesis",
    "author": "Abhishek Radadiya",
    "main": "./src/electron/main.js",
    "dependencies": {
        "@serialport/parser-byte-length": "^12.0.0",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "axios": "^1.7.6",
        "date-fns": "^3.6.0",
        "detect-port": "^1.6.1",
        "docx": "^8.5.0",
        "exceljs": "^4.4.0",
        "lodash": "^4.17.21",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.53.0",
        "react-scripts": "5.0.1",
        "serialport": "^12.0.0",
        "sqlite3": "^5.1.7",
        "web-vitals": "^2.1.4",
        "xlsx": "^0.18.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "el": "electron .",
        "dist": "electron-builder",
        "electron": "node starter.js"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "electron": "^31.1.0",
        "electron-builder": "^24.13.3",
        "electron-is-dev": "^3.0.1",
        "tailwindcss": "^3.4.4"
    },
    "build": {
        "appId": "com.peptide.synthesis",
        "productName": "Peptide_Oligo_Synthesis",
        "files": [
            "build/**/*",
            "main.js",
            "preload.js",
            "package.json"
        ],
        "publish": [
            {
                "provider": "github",
                "owner": "abhi-radadiya",
                "repo": "peptide_oligo_synthesis"
            }
        ],
        "directories": {
            "buildResources": "public"
        },
        "win": {
            "target": "nsis",
            "icon": "assets/icon.ico"
        },
        "extraResources": [
            {
                "from": "node_modules/serialport/",
                "to": "node_modules/serialport/"
            }
        ],
        "asar": true
    }
}
