        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@uidotdev/usehooks": "^2.4.1",
        "date-fns": "^3.6.0",
        "detect-port": "^1.6.1",
        "electron-updater": "^6.3.9",
        "framer-motion": "^12.4.7",
        "localforage": "^1.10.0",
        "lodash": "^4.17.21",
        "lucide-react": "^0.471.1",
        "react": "^18.3.1",
        "react-color": "^2.19.3",
        "react-dom": "^18.3.1",
        "react-draggable": "^4.4.6",
        "react-hook-form": "^7.53.0",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.27.0",
        "react-scripts": "5.0.1",
        "react-select": "^5.8.2",
        "react-window": "^1.8.10",
        "reactflow": "^11.11.4",
        "redux": "^5.0.1",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^3.1.0",
        "serialport": "^12.0.0",
